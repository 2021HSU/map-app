<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".map.MapSeoulFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.richpath.RichPathView
            android:id="@+id/ic_map_of_south_korea"

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="20dp"
            android:fillType="nonZero"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:vector="@drawable/ic_map_of_south_korea"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/goseong"
            android:layout_width="107px"
            android:layout_height="151px"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/ic_map_of_south_korea"
            app:layout_constraintEnd_toEndOf="@+id/ic_map_of_south_korea"
            app:layout_constraintHorizontal_bias="0.659"
            app:layout_constraintStart_toStartOf="@+id/ic_map_of_south_korea"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.086"
            tools:src="@tools:sample/avatars" />

        <ImageView
            android:id="@+id/haenam"
            android:layout_width="140px"
            android:layout_height="166px"
            android:background="#00CC1E1E"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/ic_map_of_south_korea"
            app:layout_constraintEnd_toEndOf="@+id/ic_map_of_south_korea"
            app:layout_constraintHorizontal_bias="0.134"
            app:layout_constraintStart_toStartOf="@+id/ic_map_of_south_korea"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.859"
            tools:src="@tools:sample/avatars" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>